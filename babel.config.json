{
  "presets": [
    "@babel/preset-typescript"
  ],
  "plugins": [
    "babel-plugin-transform-typescript-metadata",
    ["const-enum", {"transform": "constObject"}],
    ["babel-plugin-add-import-extension", 
      { "extension": "js", "replace": true }
    ],
    ["@babel/plugin-proposal-decorators", {"legacy": true}]
  ]
}